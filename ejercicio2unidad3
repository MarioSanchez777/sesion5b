X = [[100,7,3],
    [4 ,5,6],
    [7 ,8,9]]
#suma = 0
for j in range(len(X)):
    suma = 0
    for i in range(len(X)):
        suma = suma + X[i][j]
    #print(suma)
    if suma > 50:
        print("la columna ", j , "ha excedido la cantidad de 50")
    print("suma de las columnas:")
    print(suma)

